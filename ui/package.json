{
  "name": "orb-ui",
  "version": "1.0.1",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ns1labs/orb.git"
  },
  "bugs": {
    "url": "https://github.com/ns1labs/orb/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "config:env": "ts-node ./scripts/setenv.ts; ",
    "start": "npm run config:env --scripts-prepend-node-path && ng serve --proxy-config=proxy-config.json ",
    "start:withmock": "ng serve --proxy-config=proxy-config-mock.json & nodemon mock-json-server/mock-server.js --ignore 'mock-json-server/mock/sinks.json'",
    "build": "npm run config:env && ng build",
    "build:prod": "npm run config:env && npm run build -- --configuration production --aot",
    "build:prod:beta": "npm run config:env && npm run build -- --configuration beta --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint orb-ui --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/forms": "^9.0.4",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "@nebular/auth": "5.0.0",
    "@nebular/eva-icons": "5.0.0",
    "@nebular/security": "5.0.0",
    "@nebular/theme": "5.0.0",
    "@pactsafe/pactsafe-angular-sdk": "^2.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "dotenv": "^10.0.0",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "monaco-yaml": "^3.2.1",
    "nebular-icons": "1.1.0",
    "ng2-completer": "^9.0.1",
    "ngx-monaco-editor": "9.0.0",
    "ngx-mqtt": "^7.0.14",
    "node-sass": "^4.12.0",
    "normalize.css": "6.0.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.3.0",
    "style-loader": "^1.1.3",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "xng-breadcrumb": "^6.7.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "9.0.4",
    "@compodoc/compodoc": "1.0.1",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@types/d3-color": "1.0.5",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.20.42",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.1.2",
    "conventional-changelog-cli": "1.3.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "husky": "0.13.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-debounce": "^2.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "stylelint": "7.13.0",
    "ts-node": "3.2.2",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.7.5",
    "uuid": "^8.3.2"
  }
}
